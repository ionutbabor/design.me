/*===============================
ACCOUNT TEMPLATE
===============================*/
#account-page-template {

@media only screen and (max-device-width: 768px)   {

    /* Force table to not be like tables anymore */
    table, thead, tbody, th, td, tr {
      display: block!important;
    }

    /* Hide table headers (but not display: none;, for accessibility) */
    thead tr {
      position: absolute!important;
      top: -9999px;
      left: -9999px;
    }

    tr { border: 1px solid #ccc; }

    td {
      /* Behave  like a "row" */
      border: none;
      border-bottom: 1px solid #eee;
      position: relative;
      padding-left: 50%;
    }

    td:before {
      /* Now like a table header */
      position: absolute;
      /* Top/left values mimic padding */
      top: 6px;
      left: 6px;
      width: 45%;
      padding-right: 10px;
      white-space: nowrap;
    }

    /*
    Label the data
    */
    $account-labels: ('Order', 'Date', 'Payment Status', 'Fulfillment Status', 'Total');
    @for $i from 1 through length($account-labels) {
      td:nth-of-type(#{$i}):before { content: nth($account-labels, $i); }
    }
  }
}

/*===============================
CUSTOMER ORDER TEMPLATE
===============================*/

#customer-order-template {
.order-text {text-align: right;}

@media only screen and (max-device-width: 768px) {
  .order-text {
    text-align: left !important;
  }

  /* Force table to not be like tables anymore */
  table, thead, tbody, th, td, tr {
    display: block!important;
  }

  /* Hide table headers (but not display: none;, for accessibility) */
  thead tr {
    position: absolute!important;
    top: -9999px;
    left: -9999px;
  }

  tr { border: 1px solid #ccc; width: 100%; display: block!important; overflow: hidden;}

  td {
    /* Behave  like a "row" */
    border: none;
    border-bottom: 1px solid #eee;
    position: relative;
    padding-left: 50%;
  }

  td:before {
    /* Now like a table header */
    position: absolute;
    /* Top/left values mimic padding */
    top: 32%;
    left: 6px;
    width: 45%;
    padding-right: 10px;
    white-space: nowrap;
    font-weight: bold;
  }

  /*
  Label the data
  */

  tfoot {display: block!important;}
  tfoot td {
    padding-left: 0%;
    display: table-cell!important;
    float: left;
    width: 75%;
  }
  td.order-text.t2{
    float: right;
    width: 25%;
    text-align: right!important;
  }
  .order-left { font-weight: bold; }
  tfoot td:before { width: auto; }
  $customer-table-labels: ('Product', 'SKU', 'Price', 'Quantity', 'Total');
  @for $i from 1 through length($customer-table-labels) {
    td:nth-of-type(#{$i}):before { content: nth($customer-table-labels, $i); }
    tfoot td:nth-of-type(#{$i}):before { content: ""; }
  }
}
}

