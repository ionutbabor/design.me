}

.map-section__content-wrapper {
    position: relative;
    text-align: center;
    height: 100%;
    width:100%;
     @media screen and (min-width:769px){
    display:flex;
  }
 }

.map-section__content {
  position: relative;
  display: inline-block;
  width: 100%;
  padding: 15px;
  text-align: center;
  z-index: 3;
  flex-wrap:wrap;
  display:flex;
  align-content:center;
  justify-content:center;
h3{
  margin-bottom:0;

}
  // Make sure every children is on one line
  & > * {
    width: 100%;
  }
     @media screen and (min-width:769px){
    margin: 15px 0;
    min-height: 300px;
    .ie9 & {
      top: 10%;
    }
  }

  .map-section--load-error & {
    position: static;
    transform: translateY(0);
  }
}

.map-section__link {
  display: block;
  position: absolute;
  top: 0;
  left: 50%;
  max-width: none;
  width: 100%;
  height: 100%;
  z-index: 2;
  transform:translateX(-50%);
}

// Optically center map in visible area with
