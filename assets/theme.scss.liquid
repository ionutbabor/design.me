/*** Importing from "partials/parallax" ***/
/* /* ------------------------------------------------------------ *\
	Section Parallax
\* ------------------------------------------------------------ */

.section-parallax {
	position: relative;
	display: table;
	width: 100%;
	height: 500px;
	text-align: center;
	overflow: hidden;

	h2,
	.h2 {
		font-size: 2.5em;

		@media (max-width: 1365px) {
			font-size: 1.875em;
		}
	}

	.section__image-outer {
		position: absolute;
		top: 0;
		left: 0;
		overflow: hidden;
		width: 100%;
		height: 100%;
	}

	.section__image {
		position: absolute;
		top: -25%;
		left: 0;
		overflow: hidden;
		width: 100%;
		height: 150%;
		background-size: cover;
		background-position: center center;
		background-repeat: no-repeat;
		opacity: 0;
		will-change: scroll-position;
		transition: opacity .4s;

		@media (max-width: 1024px) {
			top: 0;
			height: 100%;
			background-position: center center !important;
			transform: translate3d(0px, 0px, 0px) !important;
		}
	}

	.section__inner {
		display: table-cell;
		vertical-align: middle;
	}

	.section__body {
		position: relative;
		z-index: 2;
		max-width: 60%;
		padding: 30px;
		margin: 0 auto;
		background: rgba(255,255,255,0.84);

		@media (max-width: 1023px) {
			max-width: 90%;
		}
	}

	.section__content-inner {
		margin: 0 0 2em;
		font-family: "Lato","HelveticaNeue","Helvetica Neue",sans-serif;
		font-size: 1.25em;
		line-height: 1.2;
		font-weight: 500;

		@media (max-width: 1365px) {
			font-size: 1em;
		}

		@media (max-width: 767px) {
			font-size: 0.875em;
			font-weight: normal;
		}
	}

	.search-bar {

		@media (max-width: 767px) {
			display: none;
		}
	}

	.section__bar {

		.btn--secondary {
			display: none;

			@media (max-width: 767px) {
				display: inline-block;
			}
		}
	}

	.section__actions {

		.btn {

			@media (max-width: 767px) {
				display: none;
			}
		}

		.btn--secondary {
			display: none;

			@media (max-width: 767px) {
				display: inline-block;
			}
		}
	}
}

.section-parallax--alt {
	display: block;
	height:100%;

	@media screen and (max-width:768px){
		overflow:initial;
		height:500px;

	}

	.section__image-outer {
		position:relative;
		height:500px;
		@media screen and (max-width:768px){
			height:250px;
		}
	}
	.section__image {
		position: relative;
		height:150%;
		@media screen and (max-width:768px){
			background-size:cover;
			height:100%;
		}
	}

	.section__inner {
		display: block;
		position:relative;
		width:100%;
		height:250px;
	}

	.section__body {
		max-width: none;
		height:100%;
		display:block;
		padding: 36px 30px 42px;
		opacity:1;
		background: rgba(255,255,255,1);


		@media (max-width: 1365px) {
			padding: 51px 30px 57px;
			height:100%;
		}

		@media (max-width: 768px) {

			padding: 32px 15px;	
			height:100%;
		}
	}
}

.section-parallax.loaded {

	.section__image {
		opacity: 1;
	}
}


