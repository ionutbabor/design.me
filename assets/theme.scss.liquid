/*** Importing from "partials/qty-controls" ***/
/*===============================
 QUANTITY CONTROLS
===============================*/
// Variables
$qty-border: 1px solid #aaa;

//Mixins
@mixin qty-base($width: 25px) {
  position: relative;
  width: $width;
  height: 35px;
  font-size: 1em;
  display:inline-block !important;
}
@mixin qty-controls($is-left) {
  @include qty-base;
  border: $qty-border;
  background: #f8f8f8;
  @if $is-left == true {
    margin-left: -5px;
  } @else {
    margin-right: -5px;
  }
  padding: 8px 2px;
  @media screen and (max-width:768px){
    padding: 7px 5px;
  }
  z-index:5;
}

//Styles
.quantity-wrapper{
  margin-top: 5px;
  width: 100%; 
  float: initial;
  font-size: 14px;
  @media screen and (min-width:769px){
    display: block;
    margin:0 auto;
    text-align: center;
  }
}
.qty {
  @include qty-base(40px);
  border: initial;
  border-top: $qty-border;
  border-bottom: $qty-border;
  text-align: center;
  padding: 8px 0px 8px 0px;
  vertical-align: top;
}
input.qtyplus {
  @include qty-controls(true);
}
input.qtyminus {
  @include qty-controls(false);
}

/********** Number Field Fixes **********/
input[type=number]::-webkit-inner-spin-button, 
input[type=number]::-webkit-outer-spin-button { 
  -webkit-appearance: none; 
  margin: 0; 
}
input[type=number] {
  -moz-appearance: textfield;
}

/*** END import from "partials/qty-controls" ***/

