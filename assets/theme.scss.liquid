}
.map-section--load-error {
  height: auto;
}

.map-section__wrapper {
  height: 100%;
  flex-shrink: 0;
  flex-grow: 1;
    @media screen and (min-width:769px){
  display:flex;
  flex-wrap:wrap;
  flex-direction:row;
}
}

.map-section__overlay {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0;
  z-index: 2;
}

.map-section__error {
  position: relative;
  z-index: 3;
  @media screen and (min-width:769px){
    position: absolute;
    margin: 0 2rem;
    top: 50%;
